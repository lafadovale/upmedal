  // Slider Banners Context
  
  "list-context.image-list#banners": {
    "props": {
      "height": "650",
      "images": [
        {
          "image": "assets/wood-sport-festival-banner.png",
          "description": "Wood Sport Festival"
        },
        {
          "image": "assets/grandes-guerreiros-banner.png",
          "description": "Grandes Guerreiros"
        }
      ]
    },
    "children": ["slider-layout#slider"]
  },
  "slider-layout#banners": {
    "props": {
      "blockClass": "sliderBanners",
      "infinite": true,
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      }
    }
  }
